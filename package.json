{
  "name": "netlify-deploy",
  "version": "1.0.0",
  "description": "API with express js on netlify",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p start:**",
    "start:lambda": "netlify-lambda serve ./",
