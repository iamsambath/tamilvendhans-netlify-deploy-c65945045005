    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.0",
    "netlify-lambda": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "serverless-http": "^2.7.0"
  }
